{
  "name": "@nan0web/db-fs",
  "version": "1.0.0",
  "description": "Database provider for nan•web with node:fs",
  "main": "src/index.js",
  "types": "types/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "test": "node --test",
    "clean": "rm -rf .cache/ && rm -rf dist/",
    "clean:modules": "rm -rf node_modules",
    "test:docs": "node --test src/README.md.js",
    "test:release": "node --test \"releases/**/*.test.js\"",
    "test:coverage": "node --experimental-test-coverage --test-coverage-include=\"src/**/*.js\" --test-coverage-exclude=\"src/**/*.test.js\" --test \"src/**/*.test.js\"",
    "test:coverage:collect": "nan0test coverage",
    "test:status": "nan0test status",
    "precommit": "npm test",
    "prepush": "npm test",
    "prepare": "husky"
  },
  "keywords": [
    "nanoweb",
    "database",
    "fs"
  ],
  "author": "ЯRаСлав YaRaSLove <support@yaro.page>",
  "license": "ISC",
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@nan0web/db": "workspace:*",
    "@nan0web/types": "workspace:*",
    "@nan0web/test": "workspace:*",
    "husky": "^9.1.7"
  },
  "peerDependencies": {
    "@nan0web/db": "^1.0.0",
    "@nan0web/types": "^1.0.0"
  }
}